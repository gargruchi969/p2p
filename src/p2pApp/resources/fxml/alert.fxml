<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="vBox" minHeight="122.0" prefHeight="144.0" prefWidth="562.0" style="-fx-border-color: black; -fx-border-width: 2px;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="p2pApp.p2pUi.controller.AlertController">
  <children>
    <AnchorPane fx:id="anchorPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="120.0" prefWidth="535.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="titleLabel" layoutX="21.0" layoutY="9.0" prefHeight="39.0" prefWidth="525.0" text="Label">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label fx:id="infoLabel" layoutX="28.0" layoutY="55.0" prefHeight="33.0" prefWidth="525.0" text="Label" wrapText="true" />
            <Button fx:id="cancelBtn" layoutX="290.0" layoutY="100.0" mnemonicParsing="false" onAction="#cancelClicked" prefHeight="27.0" prefWidth="78.0" text="Cancel" />
            <Button fx:id="okBtn" defaultButton="true" layoutX="180.0" layoutY="100.0" mnemonicParsing="false" onAction="#okClicked" prefHeight="27.0" prefWidth="87.0" text="Ok" />
            <TextArea fx:id="textArea" layoutX="30.0" layoutY="50.0" prefHeight="16.0" prefWidth="440.0" visible="false" />
         </children>
    </AnchorPane>
  </children>
</VBox>
